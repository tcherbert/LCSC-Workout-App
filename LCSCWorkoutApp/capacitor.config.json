{
  "appId": "com.LCSCWorkoutApp.app",
  "appName": "LCSCWorkoutApp",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "build",
  "cordova": {}

  "plugins": {
    "CapacitorFirebaseAuth": {
      "providers": ["google.com", "twitter.com", "facebook.com", "phone"],
      "languageCode": "en"
      "nativeAuth": false,
      "permissions": {
           "google": ["profile", "https://www.googleapis.com/auth/drive", ...]
       }
    }
  }
}
